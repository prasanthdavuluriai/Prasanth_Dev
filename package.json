    "serve": "^14.2.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "portfolio",
    "embedded-software",
    "automotive",
    "react",
    "fastapi",
    "mongodb"
  ],
